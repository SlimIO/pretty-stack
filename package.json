{
    "name": "@slimio/pretty-stack",
    "version": "0.4.2",
    "description": "Pretty Stack Trace to stdout in TTY",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && nyc --reporter=lcov ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/pretty-stack.git"
    },
    "keywords": [
        "javascript",
        "clean",
        "stack",
        "pretty",
        "colored"
    ],
    "files": [
        "index.d.ts",
        "index.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/pretty-stack/issues"
    },
    "homepage": "https://github.com/SlimIO/pretty-stack#readme",
    "dependencies": {
        "clean-stack": "^2.2.0",
        "kleur": "^3.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.4",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/psp": "^0.11.1",
        "ava": "^3.8.2",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "jsdoc": "^3.6.4",
        "nyc": "^15.0.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
